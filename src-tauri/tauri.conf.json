{
  "productName": "rum-runner-rhapsody",
  "version": "1.0.0",
  "identifier": "com.pdog1.rum-runner-rhapsody",
  "build": {
    "beforeDevCommand": "nuxi dev",
    "devUrl": "http://localhost:3000",
    "beforeBuildCommand": "nuxi generate",
    "frontendDist": "../dist"
  },
  "app": {"windows": [
      {
        "title": "rum-runner-rhapsody",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
